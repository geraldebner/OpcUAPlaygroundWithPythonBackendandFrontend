          "format": "time_series",
          "rawQuery": true,
          "rawSql": "-- Extract pressure curve data from JSON payload\nWITH pressure_data AS (\n  SELECT \n    \"Id\",\n    \"Name\",\n    \"CreatedAt\",\n    param->>'value' as pressure_array\n  FROM \"MeasurementSets\",\n    LATERAL jsonb_each(\"JsonPayload\"::jsonb->'groups') AS g(group_name, group_params),\n    LATERAL jsonb_array_elements(group_params) AS param\n  WHERE param->>'name' = 'pMesskurven'\n    AND param->>'value' IS NOT NULL\n  ORDER BY \"CreatedAt\" DESC\n  LIMIT 10\n),\nexpanded_points AS (\n  SELECT \n    \"CreatedAt\" + (idx || ' milliseconds')::interval as time,\n    value::float as value,\n    \"Name\" as metric\n  FROM pressure_data,\n    LATERAL jsonb_array_elements_text(pressure_array::jsonb) WITH ORDINALITY AS t(value, idx)\n)\nSELECT \n  time,\n  value,\n  metric\nFROM expanded_points\nORDER BY metric, time",
          "refId": "A"
        }
      ],
      "title": "Pressure Measurement Curves (Last 10 Datasets)",
      "type": "timeseries"
    },
