          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "-- Summary statistics for each measurement\nWITH pressure_stats AS (\n  SELECT \n    ms.\"Id\",\n    ms.\"Name\",\n    ms.\"CreatedAt\",\n    param->>'value' as pressure_array\n  FROM \"MeasurementSets\" ms,\n    LATERAL jsonb_each(ms.\"JsonPayload\"::jsonb->'groups') AS g(group_name, group_params),\n    LATERAL jsonb_array_elements(group_params) AS param\n  WHERE param->>'name' = 'pMesskurven'\n    AND param->>'value' IS NOT NULL\n)\nSELECT \n  \"Name\",\n  \"CreatedAt\",\n  jsonb_array_length(pressure_array::jsonb) as pressure_points,\n  ROUND((SELECT MIN(value::float) FROM jsonb_array_elements_text(pressure_array::jsonb) as value)::numeric, 2) as min_pressure,\n  ROUND((SELECT MAX(value::float) FROM jsonb_array_elements_text(pressure_array::jsonb) as value)::numeric, 2) as max_pressure,\n  ROUND((SELECT AVG(value::float) FROM jsonb_array_elements_text(pressure_array::jsonb) as value)::numeric, 2) as avg_pressure\nFROM pressure_stats\nORDER BY \"CreatedAt\" DESC\nLIMIT 50",
          "refId": "A"
        }
      ],
      "title": "Measurement Statistics",
